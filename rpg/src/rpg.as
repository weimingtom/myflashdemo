		private function onClick(e:MouseEvent):void {
			if (mouseY < 521 && mouseX < 1000 || mouseX > 17) {
				rolemove(role,mouseX,mouseY);
			}
		}
		private function rolemove(arole:Player,mx:int,my:int):void {
			var roleParent:DisplayObject=arole.getScene();
			var ismove:Boolean=true;
			if (my > 521 || mx > 1000) {
				ismove=false;
			} else {
				ismove=true;
			}
			if (mx < 18) {
				ismove=false;
			}
			if (ismove) {
				var clickX:Number=mx - roleParent.x;
				var clickY:Number=my - roleParent.y;
				var pathfind:PathFinding=new PathFinding(map);
				var start:Point=new Point(Math.floor(arole.x / 8) - 1,Math.floor(arole.y / 8) - 1);
				var end:Point=new Point(Math.floor(clickX / 8 - 1),Math.floor(clickY / 8) - 1);
				pathfind.path4(start,end);
				var path:Array=pathfind.optimizePath();
				if (path.length > 0 && ismove) {
					arole.moveInPath(path);
				}
				pathfind=null;
			}
		}
	}
