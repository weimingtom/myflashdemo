			private function say(str:String):void
			{
			if (str!="")	
			{  
			if (islogin!=true)
			{	
			 socket.connect("192.168.0.37",8014); 
			 socket.addEventListener(Event.CONNECT,funConnect);
			 socket.addEventListener(Event.CLOSE,funClose);
			 socket.addEventListener(ProgressEvent.SOCKET_DATA,funSocket);
			 myName=str;
			}
			else{	
				//tracemc.text = str;
				role.playerName=str;
				sendMessage("22--"+myName+"--"+str);
			}}
			else
			  addmessage("输入信息不能为空!");
			// tracemc.text+="输入信息不能为空! \n";	 
			}
			
